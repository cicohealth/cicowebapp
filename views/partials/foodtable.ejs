            $(document).on('click', '.edit', function (event) {
                console.log("edit")
                //find tbl row and its ID closest to the click, will be used to update the db
                var tbl_row = $(this).closest('tr');
                var id = tbl_row.attr('id');
                var temp_row_data = [];
